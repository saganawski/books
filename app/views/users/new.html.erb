

<%= render partial: './errors', locals: {errors: @errors} %>

<%= form_for :user, url: '/users' do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>