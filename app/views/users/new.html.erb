<% if @errors %>
	<% @errors.each do |error| %>
		<%= error %>
	<% end %>
<% end %>

<%= form_for :user, url: '/users' do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>